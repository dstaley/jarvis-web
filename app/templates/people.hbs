<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">People
        {{#link-to 'add-person' classNames="btn btn-primary pull-right"}}Add Person{{/link-to}}
        </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="row">
	<div class="col-lg-12">
	    <div class="table-responsive">
	        <table class="table table-striped table-bordered table-hover">
	            <thead>
	                <tr>
	                    <th>First Name</th>
	                    <th>Last Name</th>
	                    <th>Email</th>
	                    <th>89 Number</th>
	                    <th>Phone Number</th>
	                </tr>
	            </thead>
	            <tbody>
	                {{#each person in model.people}}
	                	<tr>
	                		<td>{{#link-to 'person' person.id}}{{person.first_name}}{{/link-to}}</td>
	                		<td>{{#link-to 'person' person.id}}{{person.last_name}}{{/link-to}}</td>
	                		<td>{{#link-to 'person' person.id}}{{person.email}}{{/link-to}}</td>
	                		<td>{{#link-to 'person' person.id}}{{person.lsu_id}}{{/link-to}}</td>
	                		<td>{{#link-to 'person' person.id}}{{person.phone_number}}{{/link-to}}</td>
	                	</tr>
	                {{/each}}
	            </tbody>
	        </table>
	    </div>
	    <!-- /.table-responsive -->
	</div>
	<!-- /.col-lg-12 -->
</div>